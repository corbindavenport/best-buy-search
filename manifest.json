{
    "manifest_version": 2,
    "name": "Best Buy Search",
    "description": "Search engine for Best Buy (unofficial)",
    "version": "1.0",
    "icons": {
        "48": "logo.svg",
        "96": "logo.svg"
    },
    "chrome_settings_overrides": {
        "search_provider": {
            "encoding": "UTF-8",
            "favicon_url": "https://bestbuy.com/favicon.ico",
            "is_default": false,
            "keyword": "bb",
            "name": "Best Buy",
            "search_url": "https://www.bestbuy.com/site/searchpage.jsp?st={searchTerms}"
        }
    },
    "browser_specific_settings": {
        "gecko": {
            "strict_min_version": "65.0"
        }
    }
}